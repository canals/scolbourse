	<queryString><![CDATA[SELECT
     dossier_d_achat.`num_dossier_achat` AS dossier_d_achat_num_dossier_achat,
     regle.`montant` AS regle_montant,
     famille.`nom_famille` AS famille_nom_famille,
     famille.`prenom_famille` AS famille_prenom_famille,
     famille.`adresse1_famille` AS famille_adresse1_famille,
     famille.`code_postal_famille` AS famille_code_postal_famille,
     famille.`ville_famille` AS famille_ville_famille,
     famille.`num_tel_famille` AS famille_num_tel_famille
FROM
     `famille` famille INNER JOIN `dossier_d_achat` dossier_d_achat ON famille.`num_famille` = dossier_d_achat.`num_dossier_achat`
     INNER JOIN `regle` regle ON dossier_d_achat.`num_dossier_achat` = regle.`num_dossier_achat`
     INNER JOIN `reglement` reglement ON regle.`code_reglement` = reglement.`code_reglement`
WHERE
     `regle`.code_reglement=4 
order by 
   dossier_d_achat_num_dossier_achat]]></queryString>

